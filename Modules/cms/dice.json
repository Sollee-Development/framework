{
    "CMS\\Model\\Text" : {
        "substitutions" : {
            "ArrayAccess" : {
                "Dice::INSTANCE" : ["MaphperLoader\\Json", "getMaphper"],
                "params" : ["cms_text"]
            }
        }
    },
    "$cms_image_maphper" : {
        "instanceOf" : "MaphperLoader\\Json",
        "call" : [
            ["getMaphper", ["cms_image"], "Dice::CHAIN_CALL"]
        ]
    },
    "CMS\\Model\\Image" : {
        "substitutions" : {
            "ArrayAccess" : {
                "Dice::INSTANCE" : ["MaphperLoader\\Json", "getMaphper"],
                "params" : ["cms_image"]
            }
        },
        "constructParams" : [
            {"Dice::INSTANCE": ["$cms_photo_model", "getFilePath"]}
        ]
    },
    "$cms_photo_model" : {
        "instanceOf" : "MVC\\Model\\FileFilter",
        "constructParams" : [
            {"Dice::INSTANCE" : "$cms_photo_model_filter" },
            "storage/cms"
        ]
    },
    "$cms_photo_model_filter" : {
        "instanceOf" : "MVC\\Model\\Sort",
        "substitutions" : {"Maphper\\Maphper" : "$cms_image_maphper"},
        "constructParams" : [
            "order_num asc"
        ]
    },
    "$cms_photo_db_saver" : {
        "instanceOf" : "MVC\\Model\\Form\\Save",
        "substitutions" : {"ArrayAccess" : "$cms_image_maphper" },
        "shared" : true
    },
    "$cms_photo_deleter" : {
        "instanceOf" : "MVC\\Model\\Form\\Delete",
        "substitutions" : {"ArrayAccess" : "$cms_image_maphper" }
    },
    "$cms_photo_file_system" : {
        "instanceOf" : "League\\Flysystem\\Filesystem",
        "constructParams" : [
            {
                "Dice::INSTANCE": "League\\Flysystem\\Adapter\\Local", "params" : [{
                "Dice::INSTANCE": ["$cms_photo_model", "getFilePath"]}]}
        ]
    },
    "$cms_photo_file_image_saver" : {
        "instanceOf" : "CMS\\Model\\Form\\SaveImage",
        "substitutions" : {
            "MVC\\Model\\Form\\SaveFile" : "$cms_photo_file_saver",
            "MVC\\Model\\Form\\Save" : "$cms_photo_db_saver",
            "ImageCache\\ImageCache" : "$cms_photo_image_cache"
        },
        "constructParams" : [
            { "Dice::INSTANCE": ["$cms_photo_model", "getFilePath"]}
        ]
    },
    "$cms_photo_file_saver" : {
        "instanceOf" : "MVC\\Model\\Form\\SaveFile",
        "substitutions" : {
            "FileUpload\\FileUpload" : {
                "Dice::INSTANCE": "$cms_photo_uploader"}
        },
        "constructParams" : [
            {
                "Dice::INSTANCE": "$cms_photo_db_saver"}
        ]
    },
    "$cms_photo_uploader" : {
        "instanceOf" : "FileUpload\\FileUpload",
        "constructParams" : [
            {
                "Dice::INSTANCE": ["Utils\\Request", "files"], "params" : ["photo_file"]},
            {
                "Dice::INSTANCE": ["Utils\\Request", "server"]},
            {
                "Dice::INSTANCE": "FileUpload\\FileNameGenerator\\Simple"}
        ],
        "call" : [
            ["addValidator", [
                {
                    "Dice::INSTANCE": "$cms_photo_validator"}
            ]],
            ["setFileSystem", [
                {
                    "Dice::INSTANCE": "FileUpload\\FileSystem\\Simple"}
            ]],
            ["setPathResolver", [
                {
                    "Dice::INSTANCE": "FileUpload\\PathResolver\\Simple", "params" : [{
                    "Dice::INSTANCE": ["$cms_photo_model", "getFilePath"]}]}
            ]]
        ]
    },
    "$cms_photo_validator" : {
        "instanceOf" : "FileUpload\\Validator\\Simple",
        "constructParams" : [
            "10M",
            ["image/jpeg", "image/gif", "image/png"]
        ]
    },
    "$cms_photo_image_cache" : {
        "instanceOf" : "ImageCache\\ImageCache",
        "constructParams" : [
            {
                "cached_image_directory" : {
                    "Dice::INSTANCE": ["$cms_photo_model", "getFilePath"]},
                "cached_image_url" : {
                    "Dice::INSTANCE": ["$cms_photo_model", "getFilePath"]},
                "quality" : {
                    "jpeg" : 60,
                    "png" : 8
                }
            }
        ]
    }
}