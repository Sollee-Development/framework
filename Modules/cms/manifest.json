{
    "authorize" : [
        {
            "routes" : ["form"],
            "instanceOf" : "Solleer\\User\\Authorize",
            "call" : ["user", []],
            "redirect" : "$not_auth"
        }
    ],
    "GET" : {
        "form" : {
            "model" : {
                "instanceOf" : "MVC\\Model\\Form\\Save",
                "substitutions" : {
                    "ArrayAccess" : {
                        "Dice::INSTANCE" : ["MaphperLoader\\Json", "getMaphper"],
                        "params" : ["cms_text"]
                    }
                }
            },
            "controller" : { "instanceOf" : "MVC\\Controller\\Form"},
            "view" : {
                "instanceOf" : "Transphporm\\Builder",
                "constructParams" : ["Layouts/layout.xml", "./view/form.tss"]
            },
            "action" : "main"
        }
    },
    "POST" : {
        "form" : {
            "inherit" : "GET",
            "action" : "submit"
        }
    }
}
