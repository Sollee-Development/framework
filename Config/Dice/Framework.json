{
    "Config\\Environment" : {
        "constructParams" : [
            "github/framework"
        ],
        "shared" : true
    },
    "Config\\Modules" : {
        "constructParams" : [
            [
                "sitetext"
            ]
        ]
    },
    "Config\\Resources" : {
        "constructParams" : [
            {
                "js" : ["node_modules/jquery/dist/jquery.js", "node_modules/jquery-validation/dist/jquery.validate.js",
                    "resources/slick/slick.js", "resources/custom.js"],
                "css" : ["resources/inuit/main.css", "resources/slick/slick.css", "resources/slick/slick-theme.css",
                    "Modules/Static/static.css"]
            }
        ]
    },
    "$404error" : {
        "instanceOf" : "Transphporm\\Builder",
        "constructParams" : ["Layouts/layout.xml", "html:header[status] { content: 404;}"]
    },
    "Config\\RouteOutput" : {
        "substitutions" : {
            "Level2\\Router\\Route" : {
                "instance" : "Level2\\Router\\Route",
                "params" : [null, {"instance" : "$404error"}, null, null]
            }
        },
        "call" : [
            ["addViewData", [{
                "config" : {
                    "environment" : {"instance" : "Config\\Environment"},
                    "resources" : {"instance" : "Config\\Resources"}
                },
                "request" : {"instance" : "Utils\\Request"}
            }]]
        ]
    },
    "$config_dice": {
        "instanceOf" : "Config\\ConfigFiles",
        "constructParams" : [
            "dice",
            ["Config/Dice/Framework.json", "Config/Dice/Config.json"]
        ]
    },
    "$config_maphper": {
        "instanceOf" : "Config\\ConfigFiles",
        "constructParams" : [
            "maphper"
        ]
    },
    "*" : {
        "substitutions" : {
            "Dice\\Dice" : {
                "instance" : ["Config\\Dice\\MultJsonFileLoader", "load"],
                "params" : [
                    {"instance": ["$config_dice", "getFiles"]}
                ]
            }
        }
    },
    "Level2\\Router\\Router" : {
        "call" : [
            ["addRule", [{"instance" : "Config\\Router\\StaticPages"}]],
            ["addRule", [{"instance" : "Solleer\\Router\\RegexModuleJson"}]],
            ["addRule", [{"instance" : "Config\\Router\\ModulePhp"}]]
        ]
    }
}
